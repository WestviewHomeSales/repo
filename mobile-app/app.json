{
  "expo": {
    "name": "Borchini Realty",
    "slug": "borchini-realty",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2563eb"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.borchinirealty.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to take photos for property inquiries.",
        "NSPhotoLibraryUsageDescription": "This app accesses your photo library to select images for property inquiries.",
        "NSLocationWhenInUseUsageDescription": "This app uses location to show nearby properties and provide directions."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2563eb"
      },
      "package": "com.borchinirealty.app",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "borchini-realty",
    "extra": {
      "eas": {
        "projectId": "your-project-id-here"
      }
    },
    "privacy": "public"
  }
}